{
  "name": "@repo/map",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./context": {
      "types": "./context/MapContext.tsx",
      "default": "./context/MapContext.tsx"
    },
    "./hooks": {
      "types": "./hooks/useMapInteractions.ts",
      "default": "./hooks/useMapInteractions.ts"
    }
  },
  "scripts": {
    "build": "tsc --skipLibCheck",
    "lint": "pnpm exec eslint . --config=eslint.config.mjs",
    "check-types": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "peerDependencies": {
    "@types/node": ">=22.13.10",
    "@types/react": ">=19.0.10",
    "@types/react-dom": ">=19.0.4",
    "eslint": ">=9.22.0",
    "react": ">=19",
    "react-dom": ">=19",
    "typescript": ">=5.8.2"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/mapbox-gl": "^2.7.18"
  },
  "dependencies": {
    "mapbox-gl": "^3.10.0",
    "react-map-gl": "^8.0.1"
  }
}
