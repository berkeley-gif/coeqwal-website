{
  "name": "coeqwal-website",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:main": "pnpm --filter main dev",
    "dev:storyline-flow": "pnpm --filter storyline-flow dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,scss,json}\"",
    "clean": "rm -rf node_modules .turbo && pnpm install && pnpm build",
    "check-types": "turbo run check-types"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sass": "^1.86.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "turbo": "^2.4.4",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": "22.x"
  }
}
